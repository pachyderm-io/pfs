{
    "pipeline": {
        "name": "sqs-spout"
    },
    "spout": {
        "overwrite": false
    },
    "transform": {
        "cmd": [ "python3", "/sqs-spout.py" ],
        "image": "svekars/sqs-spout:v5",
        "env": {
            "AWS_ACCESS_KEY_ID": "ADD_YOUR_ACCESS_KEY",
            "AWS_SECRET_ACCESS_KEY": "ADD_YOUR_ACCESS_KEY",
            "AWS_REGION": "ADD_REGION",
            "OUTPUT_PIPE": "/pfs/out",
            "S3_BUCKET": "ADD_BUCKET",
            "SQS_QUEUE_URL": "ADD_SQS_QUEUE_URL",
            "VERBOSE_LOGGING": "true"
        }
    }
}
